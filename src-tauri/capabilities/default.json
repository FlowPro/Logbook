{
  "$schema": "../node_modules/@tauri-apps/cli/schema/acl/capability.schema.json",
  "identifier": "default",
  "description": "Default capabilities for Logbuch",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "shell:allow-execute",
      "allow": [{ "name": "binaries/nmea-bridge", "sidecar": true }]
    },
    "shell:allow-kill",
    "updater:default",
    "process:default",
    "dialog:allow-save"
  ]
}
